{
  "manifestVersion": 1,
  "id": "GitClone",
  "version": "2.3.1",
  "name": "Git Repository Clone",
  "publisher": "febiunz",
  "description": "Azure DevOps extension to clone an additional Git repository and checkout a specific branch.",
  "tags": [
    "clone",
    "git",
    "azure",
    "devops",
    "pipelines"
  ],
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "icon.png"
  },
  "categories": [
    "Azure Pipelines"
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "files": [
    {
      "path": "gitclone"
    }
  ],
  "contributions": [
    {
      "id": "gitclone",
      "type": "ms.vss-distributed-task.task",
      "description": "GitClone",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "GitClone"
      }
    }
  ],
  "links": {
    "learn": {
      "uri": "https://github.com/Febiunz/AzureDevOpsExtensionGitClone/blob/master/README.md"
    },
    "repository": {
      "uri": "https://github.com/Febiunz/AzureDevOpsExtensionGitClone"
    },
    "license": {
      "uri": "https://github.com/Febiunz/AzureDevOpsExtensionGitClone/blob/master/LICENSE"
    },
    "issues": {
      "uri": "https://github.com/Febiunz/AzureDevOpsExtensionGitClone/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/Febiunz/AzureDevOpsExtensionGitClone"
  }
}